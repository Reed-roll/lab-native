{
  "expo": {
    "name": "lab-native",
    "slug": "lab-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Icon-lab.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Icon-lab.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.reeddev.labnative",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Icon-lab.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-build-properties",
      "@react-native-firebase/app",
      "@react-native-firebase/auth"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d4ab0d63-b2b0-43ef-bb19-c4b79a65a1f2"
      }
    }
  }
}
